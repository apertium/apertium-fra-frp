  </rule>

  <!-- engolar >> avaler, engueuler -->
  <macro name="verb_nopron_pron" p1="engolar" p2="avaler" p3="engueuler" />
  <rule weight="1.4">
    <match lemma="engolar" tags="vblex.*"><select lemma="engueuler"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="det.*"/>
    <or>
      <seq n="personne"/>
      <seq n="parent"/>
    </or>
  </rule>
  <rule weight="1.4">
    <match lemma="engolar" tags="vblex.*"><select lemma="engueuler"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <or>
      <match tags="np.ant.*"/>
      <match tags="np.cog.*"/>
    </or>
  </rule>

  <!-- enolyér >> lubrifier, oindre -->
  <rule weight="0.8">
    <match lemma="enolyér" tags="vblex.*"><select lemma="lubrifier"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="enolyér" tags="vblex.*"><select lemma="oindre"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="det.*"/>
    <or>
      <match n="téta" tags="n.*"/>
      <match n="man" tags="n.*"/>
      <match n="pied" tags="n.*"/>
    </or>
  </rule>

  <!-- enstalar >> installer, établir -->
  <rule weight="0.8">
    <match lemma="enstalar" tags="vblex.*"><select lemma="installer"/></match>
  </rule>

  <!-- enveyér >> envier, envoyer -->
  <rule weight="0.8">
    <match lemma="enveyér" tags="vblex.*"><select lemma="envier"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="enveyér" tags="vblex.*"><select lemma="envoyer"/></match>
    <repeat from="0" upto="6">
      <match/>
    </repeat>
    <or>
      <match lemma="lètra" tags="n.*"/>
      <match lemma="dèpèche" tags="n.*"/>
    </or>
  </rule>

  <!-- ferir >> blesser, frapper -->
  <rule weight="0.8">
    <match lemma="ferir" tags="vblex.*"><select lemma="blesser"/></match>
  </rule>

  <!-- molyér >> mouiller, impliquer -->
  <rule weight="0.8">
    <match lemma="molyér" tags="vblex.*"><select lemma="mouiller"/></match>
  </rule>

  <!-- pensar >> penser, imaginer -->
  <macro name="verb_nopron_pronque" p1="pensar" p2="penser" p3="imaginer" />

  <!-- provoquer >> provocar, mourgar -->
  <rule weight="0.8">
    <match lemma="provoquer" tags="vblex.*"><select lemma="provocar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="provoquer" tags="vblex.*"><select lemma="morgar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="det.*"/>
    <or>
      <seq n="personne"/>
      <seq n="parent"/>
    </or>
  </rule>

  <!-- rèvelyér, èvelyér, dèvelyér >> réveiller, éveiller -->
  <macro name="verb_nopron_pron" p1="rèvelyér" p2="éveiller" p3="réveiller" />
  <macro name="verb_nopron_pron" p1="èvelyér" p2="éveiller" p3="réveiller" />
  <macro name="verb_nopron_pron" p1="dèvelyér" p2="éveiller" p3="réveiller" />
  <rule weight="1.2">
    <or>
      <match lemma="rèvelyér" tags="vblex.*"><select lemma="réveiller"/></match>
      <match lemma="èvelyér" tags="vblex.*"><select lemma="réveiller"/></match>
      <match lemma="dèvelyér" tags="vblex.*"><select lemma="réveiller"/></match>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="det.*"/>
    <or>
      <seq n="personne"/>
      <seq n="parent"/>
    </or>
  </rule>

  <!-- venir, vegnir >> venir, devenir -->
  <rule weight="0.8">
    <or>
      <match lemma="venir" tags="vblex.*"><select lemma="venir"/></match>
      <match lemma="vegnir" tags="vblex.*"><select lemma="venir"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="venir" tags="vblex.*"><select lemma="devenir"/></match>
      <match lemma="vegnir" tags="vblex.*"><select lemma="devenir"/></match>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="adj.*"/>
  </rule>

