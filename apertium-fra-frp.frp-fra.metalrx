  </rule>
  <rule weight="1.0">
    <match tags="det.def.*"/>
    <match lemma="papa" tags="n.*"><select lemma="pape"/></match>
  </rule>

  <!-- par >> paire, pair -->
  <!-- pâr >> paire, pair -->
  <!-- pàr >> paire, pair -->
  <rule weight="0.8">
    <or>
      <match lemma="par" tags="n.*"><select lemma="paire"/></match>
      <match lemma="pâr" tags="n.*"><select lemma="paire"/></match>
      <match lemma="pàr" tags="n.*"><select lemma="paire"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="par" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pâr" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pàr" tags="n.*"><select lemma="pair"/></match>
    </or>
    <match lemma="de" tags="pr"/>
    <match tags="np.top"/>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="par" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pâr" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pàr" tags="n.*"><select lemma="pair"/></match>
    </or>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
    <or>
      <match lemma="rê" tags="n.*"/>
      <match lemma="royômo" tags="n.*"/>
    </or>
  </rule>

  <!-- pipèta >> pipette, cochique -->
  <rule weight="0.8">
    <match lemma="pipèta" tags="n.*"><select lemma="pipette"/></match>
  </rule>

  <!-- plâtro >> plâtre, place -->
  <rule weight="0.8">
    <match lemma="plâtro" tags="n.*"><select lemma="plâtre"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="plâtro" tags="n.*"><select lemma="place"/></match>
    <match lemma="de" tags="pr"/>
    <match tags="det.def.*"/>
    <or>
      <match tags="n.*"/>
      <match tags="num.*"/>
      <match tags="num"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="plâtro" tags="n.*"><select lemma="place"/></match>
    <match lemma="de" tags="pr"/>
    <match tags="np.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="plâtro" tags="n.*"><select lemma="place"/></match>
    <match tags="np.*"/>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="grant" tags="adj.*"/>
      <match lemma="grand" tags="adj.*"/>
    </or>
    <match lemma="plâtro" tags="n.*"><select lemma="place"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="plâtro" tags="n.*"><select lemma="place"/></match>
    <or>
      <match lemma="centrâl" tags="adj.*"/>
      <match lemma="royâl" tags="adj.*"/>
      <match lemma="royâr" tags="adj.*"/>
    </or>
  </rule>

  <!-- plomèt >> aigrette, plumet -->
  <rule weight="0.8">
    <match lemma="plomèt" tags="n.*"><select lemma="aigrette"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="plomèt" tags="n.*"><select lemma="plumet"/></match>
    <repeat from="0" upto="6">
      <match/>
    </repeat>
    <match lemma="chapeau" tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="chapeau" tags="n.*"/>
    <repeat from="0" upto="6">
      <match/>
    </repeat>
    <match lemma="plomèt" tags="n.*"><select lemma="plumet"/></match>
  </rule>

  <!-- prêsa >> prise, proie, capture -->
  <rule weight="0.8">
    <match lemma="prêsa" tags="n.*"><select lemma="prise"/></match>
  </rule>

  <!-- politico n.f. >> politique n.f., politique n.mf., politicien -->
  <rule weight="0.8">
    <match lemma="politico" tags="n.f.*"><select lemma="politique" tags="n.f.*"/></match>
  </rule>

  <!-- premiér >> premier, aîné -->
  <rule weight="0.8">
    <match lemma="premiér" tags="n.*"><select lemma="premier"/></match>
  </rule>

  <!-- règent >> instituteur, régent -->
  <rule weight="0.8">
    <match lemma="règent" tags="n.*"><select lemma="instituteur"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="règent" tags="n.*"><select lemma="régent"/></match>
    <match lemma="de" tags="pr"/>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <match tags="np.top"/>
  </rule>
  <rule weight="1.0">
    <match lemma="prince" tags="n.*"/>
    <match lemma="règent" tags="n.*"><select lemma="régent"/></match>
  </rule>

  <!-- sêson >> saison, année -->
  <rule weight="0.8">
    <match lemma="sêson" tags="n.*"><select lemma="saison"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="sêson" tags="n.*"><select lemma="année"/></match>
    <match lemma="scolèro" tags="adj.*"/>
  </rule>

  <!-- ton >> ton, thon -->
  <rule weight="0.8">
    <match lemma="ton" tags="n.*"><select lemma="ton"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="megiér" tags="vblex.*"/>
      <match lemma="mengiér" tags="vblex.*"/>
      <match lemma="gôtar" tags="vblex.*"/>
      <match lemma="couére" tags="vblex.*"/>
      <match lemma="prèparar" tags="vblex.*"/>
      <match lemma="denar" tags="vblex.*"/>
      <match lemma="dèjonar" tags="vblex.*"/>
      <match lemma="cusenar" tags="vblex.*"/>
      <match lemma="frire" tags="vblex.*"/>
      <match lemma="pèchiér" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="6">
      <match/>
    </repeat>
    <match lemma="ton" tags="n.*"><select lemma="thon"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ton" tags="n.*"><select lemma="thon"/></match>
    <repeat from="0" upto="6">
      <match/>
    </repeat>
    <or>
      <match lemma="sôça" tags="n.*"/>
      <match lemma="tomata" tags="n.*"/>
      <match lemma="tarteflla" tags="n.*"/>
      <match lemma="catrolye" tags="n.*"/>
      <match lemma="perét# de tèrra" tags="n.*"/>
      <match lemma="poma# de tèrra" tags="n.*"/>
      <match lemma="trufa" tags="n.*"/>
      <match lemma="truflla" tags="n.*"/>
      <match lemma="cortelyâjo" tags="n.*"/>
      <match lemma="jardenâjo" tags="n.*"/>
      <match lemma="hortolâjo" tags="n.*"/>
      <match lemma="vèrdura" tags="n.*"/>
      <match lemma="vèrdior" tags="n.*"/>
      <match lemma="lègumo" tags="n.*"/>
    </or>
  </rule>

  <!-- truflla, trufa >> pomme# de terre, truffe -->
  <rule weight="0.8">
    <or>
      <match lemma="truflla" tags="n.*"><select lemma="pomme# de terre"/></match>
      <match lemma="trufa" tags="n.*"><select lemma="pomme# de terre"/></match>
    </or>
  </rule>

  <!-- vielyongement >> vieillissement, obsolescence -->
  <rule weight="0.8">
    <match lemma="vielyongement" tags="n.*"><select lemma="vieillissement"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="vielyongement" tags="n.*"><select lemma="obsolescence"/></match>
    <or>
      <match lemma="programmer" tags="vblex.pp.*"/>
      <match lemma="programmé" tags="adj.*"/>
    </or>
  </rule>

  <!-- volontèro >> volontaire, bénévole -->
  <rule weight="0.8">
    <match lemma="volontèro" tags="n.*"><select lemma="volontaire"/></match>
  </rule>

<!-- **************** V **************** -->

  <!-- abosar >> démolir, écrouler -->
  <macro name="verb_nopron_pron" p1="abosar" p2="démolir" p3="écrouler" />

  <!-- apondre >> ajouter, rattacher -->
  <rule weight="0.8">
    <match lemma="apondre" tags="vblex.*"><select lemma="ajouter"/></match>
  </rule>

  <!-- aprèstar >> apprêter, préparer -->
  <macro name="verb_nopron_pron" p1="aprèstar" p2="apprêter" p3="préparer" />

  <!-- borrar >> bourrer, frapper, cogner -->
  <rule weight="0.8">
