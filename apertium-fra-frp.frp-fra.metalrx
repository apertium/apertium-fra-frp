  <rule weight="0.8">
    <match lemma="aprés" tags="pr"><select lemma="après"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="vbser.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="aprés" tags="pr"><select lemma="en train de"/></match>
    <or>
      <match tags="vbser.inf"/>
      <match tags="vbhaver.inf"/>
      <match tags="vblex.inf"/>
      <match tags="vbmod.inf"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="vbser.*"/>
    <repeat from="0" upto="2">
