         <out>
           <chunk name="pro-verb">
             <tags>
               <tag><lit-tag v="SV"/></tag>
               <tag><clip pos="3" side="tl" part="a_verb"/></tag>
             </tags>
             <lu>
               <get-case-from pos="1">
                 <lit v="en"/>
               </get-case-from>
              <lit-tag v="prn.pro.adv"/>
             </lu>
             <b/>
             <lu>
              <lit v="far"/>
              <lit-tag v="vblex.pri.p3.sg"/>
