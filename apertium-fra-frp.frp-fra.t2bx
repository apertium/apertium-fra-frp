    <rule comment="REGLA: ne SV (cette règle est pour empécher la surgénération de 'ne'">
      <pattern>
        <pattern-item n="ne"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
		    <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
        </out>
        <let><var n="fifrase"/><lit v="no"/></let>
      </action>
    </rule>

