          <match lemma="être" tags="vbser.*.p3.pl"/>
        </or>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
        <match lemma="que" tags="cnjsub"/>
      </rule>
    </def-macro>

    <!-- Dans le cas où un verbe se traduit par verbe + adv, l'adverbe doit plutôt précéder ou suivre le verbe -->
    <!-- p1 = vblex langue source, p2 = adv + vblex (langue cible), p3 = vblex + adv (langue cible) -->
    <def-macro name="verb_adv">
      <rule weight="0.8">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
    </def-macro>

    <!-- Dans le cas où un verbe se traduit par verbe + adj, l'adjectif doit s'accorder en genre et nombre. Cette macro est pour des adjectifs a 4 formes différentes -->
    <!-- p1 = vblex langue source, p2 = vblex + adj msg, p3 = vblex + adj mpl, p4 = vblex + adj fsg, p5 = vblex + adj fpl -->
    <def-macro name="verb_adj4">
      <rule weight="0.8">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="{{p1}}" tags="vblex.*.pl"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.2">
        <or>
          <match tags="n.f.sg"/>
          <match tags="adj.f.sg"/>
          <match tags="prn.tn.p3.f.sg"/>
        </or>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>
        <match lemma="{{p1}}" tags="vblex.*.sg"><select lemma="{{p4}}"/></match>
      </rule>
      <rule weight="1.2">
        <or>
          <match tags="n.f.pl"/>
          <match tags="adj.f.pl"/>
          <match tags="prn.tn.p3.f.pl"/>
        </or>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>
        <match lemma="{{p1}}" tags="vblex.*.pl"><select lemma="{{p5}}"/></match>
      </rule>
    </def-macro>

